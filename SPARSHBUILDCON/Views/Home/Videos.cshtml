@using SPARSHBUILDCON.Models;
@{
    ViewBag.Title = "Videos";
    Layout = "~/Views/Shared/_Layout.cshtml";
    UsersContext db = new UsersContext();
    var getgallery = db.ProgramGallerys.Where(aa=>aa.status==1).ToList();
}


<!--======= BANNER =========-->
  <div class="sub-banner" style="background-image: url('../Content/Home/img/lyman-banner-agents.png')">
    <div class="overlay">
      <div class="container">
        <h1>Videos</h1>
        <ol class="breadcrumb">
          <li class="pull-left">Video</li>
          <li><a href="../Home/Index">Home</a></li>
          <li class="active">Video</li>
        </ol>
      </div>
    </div>
  </div>
 
<section class="contact"> 
    
    <!--======= CONTACT INFORMATION =========-->
    <div class="contact-info">
      <div class="container"> 
        <!--======= CONTACT =========-->
          <div class="row">
		<div class="col-md-12 tittle"> 
            <img src="~/Content/Home/images/head-top.png" alt="">
		<h3>Event Videos</h3>
		</div>
              </div>
          <!--======= CERIFICATE =========-->
          <div class="row">
              @foreach (var item in getgallery)
              {
                  <div class="col-md-4 well">
                     <iframe width="100%" height="200"  src="@Url.Content(item.Video)" id="iframeid" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                  <h4 class="breadcrumb" style="color:#20853e;text-align:center">@item.Heading</h4>
              </div>
              }
             </div>
          </div>
        </div>
    </section>