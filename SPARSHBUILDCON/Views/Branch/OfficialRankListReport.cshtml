@model IEnumerable<SPARSHBUILDCON.Models.Ranktab>
@using SPARSHBUILDCON.Models
@{
    UsersContext db = new UsersContext();
    var mr = db.Members.Single(m => m.Id == 1);
    Layout = "~/Views/Shared/_BranchLayout.cshtml";
    ViewBag.Title = "OfficialRankListReport";
    var a = 0;
}

<script src="~/Scripts/print.js" type="text/javascript"></script>
<div class="my-3 my-md-5">
					<div class="container">
						<div class="page-header">
							<h4 class="page-title">@mr.rankname List</h4>
							<ol class="breadcrumb">
								<li class="breadcrumb-item"><a href="#">Dashboard</a></li>
						
							</ol>
						<a  href="@Url.Action("PrintRank")" target="_blank">	<button type="button" class="btn btn-outline-primary"><i class="fa fa-pencil mr-2"></i>Get Report In PDF</button></a>
						</div>
						<div class="row">
							<div class="col-md-12 col-lg-12">
							<div class="card">
								<div class="card-header">
									<div class="card-title">@mr.rankname List</div>
								</div>
								<div class="card-body">
                                	<div class="table-responsive">
									<table  class="table table-striped table-bordered" style="width:100%">
									  @foreach(var item in Model){
        if(a==0)
        {
  <tr>
        <th style="width:30%;text-align:center;">
           <label id="sn"><b>@mr.rankname No</b></label>
            </th>
      
        <th style="width:30%;text-align:center;">
           <label id="introducerid"><b>@mr.rankname Name</b></label>
        </th>
            
       <th style="width:30%;text-align:center;">
           <label id="Quota"><b>@mr.rankname Shortcut</b></label>
        </th>
    </tr>
        }

        a = a + 1;
    <tr style="text-align:center;">
        <td>
            @a
        </td>
      
        
        <td>
            @Html.DisplayFor(modelItem => item.RankName)
        </td>
  <td>
            @Html.DisplayFor(modelItem => item.Rankshrtcut)
        </td>
             
    </tr>
    } 

</table>
								</div>
                                </div>
								<!-- table-wrapper -->
							</div>
							<!-- section-wrapper -->

							</div>
						</div>
					</div>
				</div>
		


















