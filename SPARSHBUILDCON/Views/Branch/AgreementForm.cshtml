@using SPARSHBUILDCON.Models;
@{
    Layout = "~/Views/Shared/_BranchLayout.cshtml";
    ViewBag.Title = "AgreementForm";

    UsersContext db = new UsersContext();
    var mr = db.Members.Single(m => m.Id == 1);
}


<script type="text/javascript">
    $(document).ready(function () {
        $("#newbondid").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: "/Branch/AutoCompleteBondId",
                    type: "POST",
                    dataType: "json",
                    data: { term: request.term },
                    success: function (data) {
                        response($.map(data, function (item) {
                            return { label: item.name, value: item.newbondid };

                        }))

                    }
                })
            },
            messages: {
                noResults: "", results: ""
            }
        });
    })
</script>
<div class="my-3 my-md-5">
    <div class="container">
        <div class="page-header">
            <h4 class="page-title">Dashboard</h4>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="#">Agreement Formt</a></li>
            </ol>

        </div>
        <div class="row">
            <div class="col-12">

                <div class="card">
   <div class="card-header">
                            <h3 class="card-title">Agreement Form</h3>
   
                        </div>
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-12 col-lg-12">
                                              @using (Html.BeginForm())
                                              {
                                    <div class="form-group">
                                        
                                        <label class="form-label">@mr.custname Id </label>
                                        <div class="input-group">
                                              <input type="text" class="form-control" id="newbondid" name="newbondid" required />
                                            <span class="input-group-append">
                                               <button type="submit" class="btn btn-primary" onclick="target='_blank'">Print Agreement Form</button>     
                                            </span>
                                        </div>
                                    </div>
                                              }
                                </div>
                                    
                            </div>
                        </div>
                        
          

                </div>



            </div>

        </div>
    </div>
</div>














