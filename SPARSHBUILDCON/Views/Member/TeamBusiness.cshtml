 @model IEnumerable<SPARSHBUILDCON.Models.Downline>
@{
    ViewBag.Title = "TeamBusiness";
    Layout = "~/Views/Shared/_LayoutMember.cshtml";
    var b = 0;
}


<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header" style="background-color: #00B356;">
                <h4 class="title">Team Business Salary</h4>
            </div>
            <div class="card-content table-responsive">
                @*<a href="@Url.Action("Print_myslcincome", "Member")" target="_blank">Get Record in Pdf</a>*@
                <table class="table">
                    @foreach (var item in Model)
                    {

                        b = b + 1;
                        if (b == 1)
                        {

                        <thead class="text-primary">
                            <tr>
                                <th>Sr. No.</th>
                                <th>Member Id</th>
                                <th>Name</th>
                                <th>Amount</th>
                                <th>Income Date</th>
                                <th>Status</th>

                            </tr>
                        </thead>
                                            }

                        <tbody>
                            <tr>
                                <td>@b</td>
                                <td>@item.MemberId</td>
                                <td>@item.levelincome</td>
                                <td>@item.JoinDate.ToString("dd/MM/yyyy")</td>

                            </tr>
                        </tbody>
                                         
                                        }
                </table>
            </div>
        </div>
    </div>
</div>

