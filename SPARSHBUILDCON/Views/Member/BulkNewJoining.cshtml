<script type="text/javascript" language="javascript">
    function JavascriptFunction() {
        var url = '@Url.Action("PostMethod", "Admin")';
        $("#divLoading").show();
        $.post(url, null,
                function (data) {
                    $("#PID")[0].innerHTML = data;
                    $("#divLoading").hide();
                });
    }
</script>

<script>
    function introname(check_id) {

        debugger

        var Introducer1 = $("#Introducer1").val();

        var url = '@Url.Action("introname", "Home")';

        if (Introducer1.length > 0) {
            $.get(url, { Introducer1: Introducer1 }, function data(t) {

                if (t.length > 0) {
                    $("#introname").text(t);
                }
                else {
                    $("#introname").text("");
                }
            });
        }
        else {
            $("#introname").text("");
        }

        }
</script>

