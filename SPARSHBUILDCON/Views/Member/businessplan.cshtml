@using SPARSHBUILDCON.Models;
@{
    ViewBag.Title = "businessplan";
    Layout = "~/Views/Shared/_member_new_layout.cshtml";
    UsersContext db = new UsersContext();
    var comp = db.CompanyInfos.Single(a => a.Id == 1);
    var cust = db.AgentDetails.Single(a => a.NewAgentId == User.Identity.Name);
}




<div id="main-content">
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">
                <h3 class="page-title">Business Plan
                </h3>
                <i style="font-weight: 800; color: green; text-align: center;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; @ViewData["msg"] </i>
                <ul class="breadcrumb">
                    <li><a href="~/Member/Dashboard"><i class="icon-home"></i></a><span class="divider">&nbsp;</span>

                    </li>
                    <li><a href="#">@comp.CompanyName</a><span class="divider">&nbsp;</span> </li>
                    <li><a href="#">Business Plan</a><span class="divider-last">&nbsp;</span></li>
                </ul>
            </div>
        </div>
      <div class="row-fluid">
        <div class="span12">
           <div class="widget-title">
                    <h4>
                        <i class="icon-reorder"></i>All Business Plan Detail</h4>  </div>
                        </div>
                        </div>
         <br />        
                           <div class="col-lg-12" >
        <center><div class="col-sm-12" style="height:1000px; overflow-y:scroll">
            
            @foreach(var i in db.BussinessPlanImages.Where(aa=>aa.Status!=0).ToList())
            {     
            <img src="@Url.Content(i.Images)" style="width:80%;height:70%" class="img-responsive"/><br />
            }
           
        </div></center>
            </div>
        
</div>
    </div>
