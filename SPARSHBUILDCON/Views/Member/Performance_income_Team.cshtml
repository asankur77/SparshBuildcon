@using PagedList.Mvc
@model  PagedList.IPagedList<SPARSHBUILDCON.Models.Memberlist>
@using SPARSHBUILDCON.Models;
@{
    ViewBag.Title = "Performancebonus_income";
    Layout = "~/Views/Shared/_member_new_layout.cshtml";
    var b = 1;
    int counting = Convert.ToInt32(ViewData["lastpage"]);
    int pageno = Convert.ToInt32(ViewData["pageno"]);
    int gets = Convert.ToInt32(ViewData["srno"]);
    int sr = gets + 1;
    int starts = gets + 1;
    UsersContext db = new UsersContext();
    var name = db.AgentDetails.Single(aa => aa.NewAgentId == User.Identity.Name);
}

<div id="main-content">
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">
                <h3 class="page-title">Team Performance Bonus Income
                </h3>
                <i style="font-weight: 800; color: green; text-align: center;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; @ViewData["msg"] </i>
                <ul class="breadcrumb">
                    <li><a href="~/Member/Dashboard"><i class="icon-home"></i></a><span class="divider">&nbsp;</span>

                    </li>
                    <li><a href="#">DYNAMIC WAY REAL ESTATE PVT LTD</a><span class="divider">&nbsp;</span> </li>
                    <li><a href="#">Performance Bonus Income</a><span class="divider-last">&nbsp;</span></li>
                </ul>
            </div>
        </div>

        <div class="row-fluid">
            <div class="row-fluid">
                <div class="span12">
                    <div class="widget-title">
                        <h4>
                            <i class="icon-reorder"></i>Direct Performance Bonus Income </h4>
                    </div>
                </div>
            </div>
            <br />
            <div class="col-lg-12">
                <div class="col-sm-12">

                    <div class="card-box table-responsive">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered table-hover dataTables-example">

                                @if (b == 1)
                                {
                                    
                                    <thead class="text-primary">
                                        <tr>
                                            <th>Sr. No.</th>
                                            <th>Member Id</th>
                                            <th>Name</th>
                                            <th>Amount</th>
                                            <th>Start Date</th>
                                            <th>Last Date</th>
                                            <th>Status</th>

                                        </tr>
                                    </thead>
                                }
                                <tbody>
                                    <tr>
                                        @if (pageno == 1)
                                        {
                                            <td>@b</td>
                                        }
                                        else
                                        {<td>@sr</td>
                                        }
                                        <td>@User.Identity.Name</td>
                                        <td>@name.name</td>
                                        <td>@Model.Select(a => a.totalamt).DefaultIfEmpty(0).Sum()</td>
                                        <td>@Model.Select(a => a.JoinDate).Min()</td>
                                        <td>@Model.Select(a => a.JoinDate).Max()</td>
                                        <td>Paid</td>
                                    </tr>
                                </tbody>

                            </table>
                            
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row-fluid">
            <div class="row-fluid">
                <div class="span12">
                    <div class="widget-title">
                        <h4>
                            <i class="icon-reorder"></i>Team Performance Bonus Income </h4>
                    </div>
                </div>
            </div>
            <br />
            <div class="col-lg-12">
                <div class="col-sm-12">

                    <div class="card-box table-responsive">
                        <div class="table-responsive">

                            <p>Coming Soon ...</p>

                            <center> <div id="container" >
        <p></p>
        <p></p>
     
    </div></center>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>



