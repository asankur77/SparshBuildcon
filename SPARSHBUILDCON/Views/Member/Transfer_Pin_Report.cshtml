@using SPARSHBUILDCON.Models;
@{
    ViewBag.Title = "Transfer_Pin_Report";
    Layout = "~/Views/Shared/_member_new_layout.cshtml";
    UsersContext db = new UsersContext();
    var cpin = db.Transfer_Pins.Where(x => x.srno != 0 && x.Transfer_By == User.Identity.Name).ToList();
    int a = 1;
}

<div id="main-content">
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">
                <h3 class="page-title">Transfer Pin History
                </h3>
                <i style="font-weight: 800; color: green; text-align: center;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; @ViewData["msg"] </i>
                <ul class="breadcrumb">
                    <li><a href="~/Member/Dashboard"><i class="icon-home"></i></a><span class="divider">&nbsp;</span>

                    </li>
                    <li><a href="#">DYNAMIC WAY REAL ESTATE PVT LTD</a><span class="divider">&nbsp;</span> </li>
                    <li><a href="#">Transfer Pin History</a><span class="divider-last">&nbsp;</span></li>
                </ul>
            </div>
        </div>
        <div class="row-fluid">
            <div class="span12">
                <div class="widget-title">
                    <h4>
                        <i class="icon-reorder"></i>All Transfer Pin Detail</h4>
                </div>
            </div>
        </div>
        <br />
        <div class="col-lg-12">
            <div class="col-sm-12">
                <div class="card-box table-responsive">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover dataTables-example">
                            <thead>
                                <tr>
                                    <th>SrNo</th>
                                    <th>Transfer By</th>
                                    <th>Transfer To</th>
                                    <th>Total PIN</th>
                                    <th>Date</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var i in cpin)
                                {
                                     
                                    <tr>
                                        <td>@a</td>
                                        <td>@i.Transfer_By</td>
                                        <td>@i.Transfer_To</td>
                                        <td>@i.Total_Pin</td>
                                        <td>@i.Date</td>
                                    </tr>
                                        a = a + 1;
                                    }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>