@model IEnumerable<SPARSHBUILDCON.Models.ranList>
@using SPARSHBUILDCON.Models
@{
    UsersContext db = new UsersContext();
    var mr = db.Members.Single(m => m.Id == 1);
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.Title = "RankList";
    var a = 0;
}
<div class="my-3 my-md-5">
					<div class="container" id="divid">
						<div class="page-header">
							<h4 class="page-title">@mr.rankname List</h4>
					
						</div>
						<div class="row">
							<div class="col-md-12 col-lg-12">
							<div class="card">
								<div class="card-header">
									<div class="card-title">@mr.rankname List</div>
								</div>
								<div class="card-body">
                                	<div class="table-responsive">
									<table  class="table table-striped table-bordered" style="width:100%">
 @foreach(var item in Model){
        if(a==0)
        {
  <tr>
        <th style="width:30%;text-align:center;">
           <label id="sn"><b>@mr.rankname No</b></label>
            </th>
      
        <th style="width:30%;text-align:center;">
           <label id="introducerid"><b>@mr.rankname Name</b></label>
        </th>
            
       <th style="width:30%;text-align:center;">
           <label id="Quota"><b>Self Quota</b></label>
        </th>
    </tr>
        }

        a = a + 1;
    <tr style="text-align:center;">
        <td>
            @a
        </td>
      
        
        <td>
            @Html.DisplayFor(modelItem => item.rankname)
        </td>
  <td>
            @Html.DisplayFor(modelItem => item.selfquota)
        </td>
             
    </tr>
    } 

</table>
    @if (a > 0)
    {
         <a href="@Url.Action("PrintRank")" target="_blank">Get Report In PDF</a>
    }  

                                        </div>
                                    </div>
                                </div>
                                </div>
                            </div>
                        </div>
    </div>


















































